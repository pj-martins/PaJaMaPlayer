<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
			 xmlns:c="clr-namespace:PaJaMaPlayer"
             x:Class="PaJaMaPlayer.PlayPage">
	<ContentPage.ToolbarItems>
		<ToolbarItem x:Name="toolPlayStop" Clicked="playStop_Clicked" />
		<ToolbarItem Icon="ic_list_white_24dp.png" Clicked="playlist_Clicked" />
	</ContentPage.ToolbarItems>
	<ContentPage.Content>
		<StackLayout>
			<Label x:Name="lblName" Margin="8,8,0,0"></Label>
			<Label x:Name="lblTrack" Margin="8,0,0,0"></Label>
			<StackLayout x:Name="layoutPlaylists"></StackLayout>
			<StackLayout x:Name="layoutEqualizer"></StackLayout>
		</StackLayout>
	</ContentPage.Content>
</ContentPage>